<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Example: set default website name and company details -->
  <record id="gm_company" model="res.company">
    <field name="name">GreenMotive</field>
    <!-- logo can be set via binary in real use; keep blank here -->
  </record>

  <!-- Assign default brand to default website if it exists -->
  <function model="website" name="search">
    <value eval="[]"/>
  </function>
  
  <record id="website.default_website" model="website">
    <field name="brand_id" ref="deployable_brand_theme.brand_greenmotive"/>
  </record>
</odoo>